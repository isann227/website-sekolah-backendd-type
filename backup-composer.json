{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "alibabacloud/onsmqtt-20200420": "1.0.2",
        "alphasnow/aliyun-oss-appserver": "^0.3.1",
        "alphasnow/aliyun-oss-laravel": "*",
        "anhskohbo/no-captcha": "^3.3",
        "apility/laravel-fcm": "^1.4",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "brian2694/laravel-toastr": "^5.56",
        "doctrine/dbal": "^3.2",
        "doku/jokul-php-library": "^2.0",
        "edamov/pushok": "^0.15.6",
        "firebase/php-jwt": "^6.5",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "jeremykenedy/laravel-logger": "^6.0",
        "laravel/framework": "^8.75",
        "laravel/passport": "^10.2",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.5",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.3",
        "maatwebsite/excel": "^3.1",
        "maennchen/zipstream-php": "^2.1",
        "phpoffice/phpspreadsheet": "1.23.0",
        "pragmarx/google2fa-laravel": "^2.0",
        "propaganistas/laravel-phone": "^4.3",
        "psr/simple-cache": "1.0",
        "pusher/pusher-php-server": "^7.2",
        "realrashid/sweet-alert": "^5.0",
        "seshac/otp-generator": "^0.0.7",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-backup": "*",
        "spatie/laravel-permission": "^5.4",
        "theanik/laravel-more-command": "^1.2",
        "web-token/jwt-core": "^3.2",
        "yajra/laravel-datatables-oracle": "^9.0",
        "lcobucci/clock":"^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/envoy": "^2.8",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
